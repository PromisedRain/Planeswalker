; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Planeswalker"
config/version="0.1"
config/tags=PackedStringArray("indevelopment")
run/main_scene="res://src/scenes/main.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/show_image=false
config/icon="res://icon.svg"

[autoload]

Utils="*res://src/lib/utils.gd"
SignalManager="*res://src/scenes/managers/signalManager.gd"
SaveManager="*res://src/scenes/managers/saveManager.gd"
LevelManager="*res://src/scenes/managers/levelManager.tscn"
GlobalManager="*res://src/scenes/managers/globalManager.gd"
InteractionManager="*res://src/scenes/managers/interactionManager.tscn"
SceneTransitionManager="*res://src/scenes/managers/sceneTransitionManager.tscn"
UiManager="*res://src/scenes/managers/uiManager.tscn"
ProgressionManager="*res://src/scenes/managers/progressionManager.gd"

[display]

window/size/viewport_width=320
window/size/viewport_height=180
window/size/always_on_top=true
window/size/window_width_override=960
window/size/window_height_override=540
window/stretch/mode="viewport"
window/stretch/aspect="expand"

[file_customization]

folder_colors={
"res://src/addons/": "gray",
"res://src/builds/": "gray",
"res://src/common/": "gray",
"res://src/components/": "red",
"res://src/gui/": "green",
"res://src/lib/": "pink",
"res://src/media/": "purple",
"res://src/resources/": "yellow",
"res://src/scenes/": "blue",
"res://src/shared/": "gray",
"res://src/testing/": "gray"
}

